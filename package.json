{
  "name": "cvdecklist",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android-dev": "react-native run-android",
    "ios-8-plus": "react-native run-ios --simulator=\"iPhone 8 Plus\"",
    "ios-11-promax": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios-production": "react-native run-ios --simulator=\"iPhone 8 Plus iOs 12.4\"",
    "ios-release": "react-native run-ios --configuration Release --device \"Đàm Trung Đức’s iPhone\"",
    "ios-local": "ENVFILE=.env.local react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios-secondary": "react-native run-ios --simulator=\"iPhone 11 Pro Max\" --port=8088",
    "start-secondary": "react-native start --port=8088",
    "generate-component": "cd App/Components && echo `import React from \"react\";'\n'`> ${FILE}.js && cd Styles && echo > ${FILE}Style.js && cd ../../..",
    "generate-container": "cd App/Containers && echo > ${FILE}.js && cd Styles && echo > ${FILE}Style.js && cd ../../.."
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-firebase/app": "^6.3.4",
    "@react-native-firebase/crashlytics": "^6.3.4",
    "@react-navigation/bottom-tabs": "^5.1.1",
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.1.1",
    "@types/i18n-js": "^3.0.1",
    "axios": "^0.19.2",
    "i18n-js": "^3.5.1",
    "lodash": "^4.17.15",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-code-push": "^6.2.0",
    "react-native-config": "^0.12.0",
    "react-native-device-info": "^5.5.3",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-local-resource": "^0.1.6",
    "react-native-localize": "^1.3.3",
    "react-native-onesignal": "^3.5.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.19",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
